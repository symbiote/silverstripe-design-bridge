{
    "name": "symbiote/silverstripe-design-bridge",
    "description": "A module that allows viewing components in isolation",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "symbiote"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jake Bentvelzen",
            "email": "jake@symbiote.com.au"
        }
    ],
    "scripts": {
        "phpcbf": "phpcbf -n src/ tests/"
    },
    "require": {
        "silverstripe/cms": "~3.6",
        "silbinarywolf/silverstripe-components": ">=0.3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\DesignBridge\\": "src/",
            "Symbiote\\DesignBridge\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\DesignBridge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "installer-name": "design-bridge"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
